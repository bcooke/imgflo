{
  "name": "imgflo-d3",
  "version": "0.1.0",
  "description": "D3 data visualization generator for imgflo using server-side rendering",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "imgflo",
    "d3",
    "data-visualization",
    "charts",
    "svg",
    "image-generation"
  ],
  "author": "Brett Cooke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcooke/imgflo.git",
    "directory": "packages/imgflo-d3"
  },
  "peerDependencies": {
    "imgflo": "workspace:*"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "jsdom": "^25.0.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "imgflo": "workspace:*",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
